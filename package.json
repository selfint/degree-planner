{
	"name": "degree-planner",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": "20.x"
	},
	"scripts": {
		"dev": "vite dev",
		"generate-catalogs": "node scripts/generateCatalogsManifest.js && node scripts/generateCatalogsHeader.js && node scripts/generateRequirementsData.mjs",
		"build": "npm run generate-catalogs && vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"script:db": "tsx src/lib/server/scripts/db.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-vercel": "^6.3.0",
		"@sveltejs/kit": "^2.11.1",
		"@types/eslint": "^9.6.1",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^20.19.28",
		"@types/sortablejs": "^1.15.8",
		"@typescript-eslint/eslint-plugin": "^8.18.0",
		"@typescript-eslint/parser": "^8.18.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"postcss": "^8.4.49",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"svelte": "^5",
		"svelte-check": "^4.1.1",
		"tailwindcss": "^3.4.16",
		"tslib": "^2.8.1",
		"tsx": "^4.19.2",
		"typescript": "^5.7.2",
		"vite": "^6.0.3",
		"vitest": "^2.1.8"
	},
	"type": "module",
	"dependencies": {
		"@vercel/analytics": "^1.4.1",
		"@vercel/speed-insights": "^1.1.0",
		"firebase": "^11.1.0",
		"pdfjs-dist": "^4.9.155",
		"sortablejs": "^1.15.6"
	}
}
